<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="~/js/add-account.js"></script>
<fieldset>
	<legend>Add Account</legend>
	<label for="nickname">Nickname:
		<input id="nickname" type="text" />
	</label>
	<label>Type:
		<input name="account-type" type="radio" value="checking"/>Checking
		<input name="account-type" type="radio" value="savings"/>Savings
	</label>
	<label for="balance">Balance:
		<input id="balance" type="text" />
	</label>
	<button id="submit" onclick="submitAccount()">Save</button>
	<button id="clear" onclick="clearForm()">Clear</button>
</fieldset>